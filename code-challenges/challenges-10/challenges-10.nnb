            "source": [
                "function birthdayCakeCandles(candles) {\n    let max=0;\n    let sum=0;\n    for(let i=0;i<candles.length;i++)\n    {\n        if(candles[i]>max)\n        {max=candles[i];\n        sum=1;}\n        else if(candles[i]==max)\n        sum++;\n        \n    }\nreturn sum;\n}"
