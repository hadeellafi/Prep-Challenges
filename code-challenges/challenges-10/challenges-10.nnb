            "source": [
                "function compareTriplets(a, b) {\n    let arr=[0,0]\n    for(let i=0;i<a.length;i++)\n    {\n        if(a[i]>b[i])\n        arr[0]++;\n        if(a[i]<b[i])\n        arr[1]++;\n    }\n    return arr;\n    }"
